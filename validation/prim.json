{
  "$schema": "schema/index.json",
  "specs": [
    {
      "name": "Prim",
      "modules": [
        "Array",
        "Blob",
        "Bool",
        "Char",
        "Option",
        "Principal",
        "Text"
      ],
      "functions": ["equal"]
    },
    {
      "name": "Number",
      "extends": ["Prim"],
      "modules": ["Float"],
      "functions": []
    },
    {
      "name": "IntNat",
      "extends": ["Number"],
      "modules": [
        "Int",
        "Int8",
        "Int16",
        "Int32",
        "Int64",
        "Nat",
        "Nat8",
        "Nat16",
        "Nat32",
        "Nat64"
      ],
      "functions": ["range", "rangeInclusive", "min", "max"]
    }
  ]
}
